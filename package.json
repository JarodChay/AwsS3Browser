{
    "author": "Jarod Chay",
    "name": "aws-s3-browser",
    "version": "1.0.0",
    "description": "JavaScript browser for files stored on AWS S3",
    "scripts": {
        "start": "npm-run-all open:src",
        "open:src": "node buildScripts/srcServer.js",
        "localtunnel": "lt --port 3001 --subdomain chay",
        "share": "npm-run-all --parallel open:src localtunnel"
    },
    "dependencies": {
        "express": "^4.16.3",
        "npm-run-all": "^4.1.3",
        "open": "0.0.5"
    }
}
